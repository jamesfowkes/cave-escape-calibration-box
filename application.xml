<?xml version="1.0"?>

<board type="Arduino Nano" name="Calibration Box"> <info>An ethernet controlled switch puzzle</info>

<devices>
    <device name="Ethernet" type="ENC28J60">
        <setting id="cs_pin" value="10"></setting>
    </device>

    <device name="Part1Input" type="Debounced Input">
            <setting id="pin" name="Input" value="6"/>
            <setting id="debounce_time" value="50"/>
            <setting id="pullup" value="false"/>
    </device>

    <device name="Part1Output" type="Digital Output">
            <setting id="pin" name="Input" value="7"/>
            <setting id="debounce_time" value="50"/>
            <setting id="pullup" value="false"/>
    </device>

    <device name="Part2Input" type="Debounced Input">
            <setting id="pin" name="Input" value="4"/>
            <setting id="debounce_time" value="50"/>
            <setting id="pullup" value="false"/>
    </device>

    <device name="Part2Output" type="Digital Output">
            <setting id="pin" name="Input" value="5"/>
            <setting id="debounce_time" value="50"/>
            <setting id="pullup" value="false"/>
    </device>

</devices>

<modules>
    <module>HTTP Get Server</module>
    <module>Task</module>
</modules>

<custom_code>
    <file>application.cpp</file>
</custom_code>

<defines>
    <define>ENC28J60_BUFFER_SIZE 128</define>
</defines>

<logging>
    <log_module>APPLICATION</log_module>
</logging>

<raat protocol="url"></raat>
</board>